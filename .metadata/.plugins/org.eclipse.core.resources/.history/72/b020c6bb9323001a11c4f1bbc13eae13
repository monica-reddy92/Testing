package com.CoreJava;

public class for_loop {

	public static void main(String[] args) 
	
	{
		//simple and nested for loop
		
		for(int i=1;i<=5;i++)
		{
			for(int j=1;j<=i;j++)
			{
				System.out.print("*"+" ");

			}
			System.out.println();
		}
		
		//for-each
		
		System.out.println("###########");
		char arr[]= {'a','b','c','d','e'};

		for (char c:arr)
		{
			System.out.print(c+" ");
		}
		}
	}


